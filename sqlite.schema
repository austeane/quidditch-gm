CREATE TABLE players(
	id integer primary key autoincrement,
	name text,
	gender text,
	length real,
	team text,
	beater integer,
	chaser integer,
	keeper integer,
	seeker integer,
	rank real,
	drafted text);

CREATE TABLE ranks(
	id integer primary key autoincrement,
	playerid integer,
	rank real,
	confidence real,
	weightedRank real);